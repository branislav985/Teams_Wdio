{
  "name": "Teams_Wdio",
  "type": "module",
  "devDependencies": {
    "@testing-library/webdriverio": "^3.2.1",
    "@wdio/cli": "^9.19.2",
    "@wdio/cucumber-framework": "^9.19.2",
    "@wdio/json-reporter": "^9.19.2",
    "@wdio/local-runner": "^9.19.2",
    "@wdio/spec-reporter": "^9.19.2",
    "@wdio/visual-service": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "wdio-cucumberjs-json-reporter": "^6.0.1",
    "wdio-intercept-service": "^4.4.1",
    "wdio-json-html-reporter": "^1.5.14",
    "wdio-wait-for": "^3.1.0"
  },
  "scripts": {
    "wdio": "wdio run ./wdio.conf.js",
    "test": "wdio run ./wdio.conf.js",
    "test:smoke": "wdio run ./wdio.conf.js --cucumberOpts.tagExpression='@smoke'",
    "test:regression": "wdio run ./wdio.conf.js --cucumberOpts.tagExpression='@regression'",
    "test:parallel": "wdio run ./wdio.conf.js --maxInstances=5",
    "report": "allure generate allure-results --clean -o allure-report && allure open",
    "format": "prettier --write features/ --ext .js --fix"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
